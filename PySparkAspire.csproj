<Project Sdk="Aspire.AppHost.Sdk/13.1.0">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <UserSecretsId>4e45b7ae-1163-496e-bd0a-317c74d97a04</UserSecretsId>
    <!-- Allow Cosmos emulator preview APIs during development -->
    <NoWarn>$(NoWarn);ASPIRECOSMOSDB001</NoWarn>
    <!-- If you treat warnings as errors, ensure this isn't promoted to an error -->
    <WarningsNotAsErrors>$(WarningsNotAsErrors);ASPIRECOSMOSDB001</WarningsNotAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aspire.Hosting.Azure.CosmosDB" Version="13.1.0" />
  </ItemGroup>

</Project>
